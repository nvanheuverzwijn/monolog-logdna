{
    "name": "nvanheuverzwijn/monolog-logdna",
    "description": "Mezmo/Logdna handler for Monolog",
    "keywords": ["log", "logging", "psr-3", "logdna", "mezmo"],
    "homepage": "http://github.com/nvanheuverzwijn/monolog-logdna",
    "type": "library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Nicolas Vanheuverzwijn",
            "email": "nicolas.vanheu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "monolog/monolog": ">=3.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.3"
    },
    "autoload": {
        "psr-4": {"Zwijn\\Monolog\\": "src/Monolog"}
    },
    "autoload-dev": {
        "psr-4": {"Zwijn\\Monolog\\": "tests/Monolog"}
    },
    "scripts": {
        "test": "phpunit"
    }
}
